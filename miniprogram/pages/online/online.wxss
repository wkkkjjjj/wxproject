/* pages/online/online.wxss */
page {
  height: 100%;
}

.coat {
  position: relative;
  background-color: #cfcfc4;
  width: 100%;
  height: 100%;
  padding: 0;
  margin: 0;
}

.background {
  position:absolute;
  width: 100%;
  height: 100%;
}

.load {
    margin:0;
    padding:50rpx;
    position:absolute;
    width:80%;
    height:30%;
    box-sizing: border-box;
    left:10%;
    text-align:center;
    top:35%;
    border-radius: 3%;
    background-color:rgb(225,225,0,0.3);
}

.find {
    font-weight:bold;
    font-size:46rpx;
    color:rgb(0,0,0);
}

.tap {
    position:relative;
    margin:100rpx;
    left:30rpx;
}